= semantic_form_for(@attribute) do |f|
  = f.input :name, :label => 'Name'
  %br/
  = f.input :description, :as => :ckeditor, :label => 'Description'
  %br/
  = f.input :rooms, :as => :check_boxes, :label => 'Choose Rooms for this Technology'
  %br/
  = f.input :repositories, :as => :check_boxes, :label => 'Available Libraries/Archives for this Technology'
  = f.actions do
    = f.action :submit, :as => :button, :button_html => {:class => 'btn btn-primary'}
